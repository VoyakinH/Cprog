#!/bin/bash
cd func_tests
touch empty_file.txt
for ((i=1; i<5; i++))
do
echo test/#$i:
file_in=in_0$i.txt
file_out=empty_file.txt
touch results.txt
../app.exe < $file_in > results.txt
diff -s results.txt $file_out
done
for ((i=5; i<10; i++))
do
echo test/#$i:
file_in=in_0$i.txt
file_out=out_0$i.txt
rm results.txt
touch results.txt
../app.exe < $file_in > results.txt
diff -s results.txt $file_out
done
for ((i=10; i<17; i++))
do
echo test/#$i:
file_in=in_$i.txt
file_out=out_$i.txt
rm results.txt
touch results.txt
../app.exe < $file_in > results.txt
diff -s results.txt $file_out
done
rm empty_file.txt
rm results.txt